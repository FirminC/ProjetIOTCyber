<h1>Welcome</h1>

<% if logged_in? %>
    <p>You are Logged In, <%= current_user.username %></p>

    <%= button_to "Logout", logout_path, method: :get %>
<% else %>
    <%= button_to "Login", login_path, method: :get %>
<%end%>

<% if is_admin? %>
    <%= button_to "Users", users_path, method: :get %>
<% end %>